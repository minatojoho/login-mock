<h1>Home</h1>
<% if current_user %>
  <div><%= link_to('Users', users_path) %></div>
  <div><%= link_to('Logout', logout_path, method: :delete) %></div>
<% else %>
  <div><%= link_to('Login', login_path, method: :post) %></div>
  <div><%= link_to('Create An Account', new_user_path) %></div>
<% end %>
